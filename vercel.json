{
  "version": 2,
  "rewrites": [
    { "source": "/generate_204", "destination": "/api/online" },
    { "source": "/proxy/(.*)", "destination": "/api/proxy?url=$1" },
    { "source": "/sw.js", "destination": "/sw.js" },
    { "source": "/inject.js", "destination": "/inject.js" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
